{
  "name": "CoreSwift",
  "version": "1.0.0",
  "description": "Framework for Cypress using cucumber",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rm cypress/cucumber-json/* || true",
    "test": "node_modules\\.bin\\cypress run",
    "chromeHeaded": "npm run clean:reports && npx cypress run --browser chrome --headed",
    "chromeHeadless": "npm run clean:reports && npx cypress run --browser chrome --headless"
  },
  "author": "XYZ",
  "license": "ISC",
  "devDependencies": {
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@types/lodash": "^4.14.182",
    "axios": "^0.21.1",
    "cypress": "^8.4.1",
    "cypress-browser-permissions": "^1.1.0",
    "cypress-cucumber-preprocessor": "^4.2.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-xpath": "^1.6.2",
    "dayjs": "^1.10.4",
    "eslint": "^8.11.0",
    "eslint-plugin-cypress": "^2.12.1",
    "faker": "^5.5.1",
    "fs": "0.0.1-security",
    "fs-extra": "^9.1.0",
    "fs-js": "^1.0.6",
    "mocha": "^8.3.2",
    "mochawesome": "^6.3.1",
    "mochawesome-merge": "^4.2.0",
    "moment": "^2.29.4",
    "multiple-cucumber-html-reporter": "^1.21.6",
    "node-xlsx": "^0.21.0",
    "path": "^0.12.7"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
